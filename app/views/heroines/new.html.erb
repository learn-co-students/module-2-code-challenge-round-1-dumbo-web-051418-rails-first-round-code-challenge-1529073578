<h1>Create Heroine: </h1>
<%= form_for @heroine do |f| %>
  <strong><%= f.label :name, "Name" %>:
  <%= f.text_field :name %><br /></strong>
  <br>
  <strong><%= f.label :super_name, "Super Name" %></strong>
  <%= f.text_field :super_name %><br />
  <br>
  <%= f.collection_select(:power_id, Power.all, :id, :name) %>
  
  <%= f.submit "Create Heroine" %>
<% end %>
