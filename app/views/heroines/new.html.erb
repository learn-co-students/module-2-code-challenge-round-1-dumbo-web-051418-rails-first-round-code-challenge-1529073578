
<%= form_for @heroine do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br />

  <%= f.label :super_name %>
  <%= f.text_field :super_name %><br />

  <%= f.label :power %>
<%=   collection_select(:power, :power_id, Power.all, :id, :name, prompt: true) %>
  <!-- the collection select seems like it's not passing the correct parameters to the created heroine-->

  <%= f.submit %>
<% end %>
