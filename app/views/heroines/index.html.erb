<h2>All Heroines</h2>

<br>
<!-- work in progress -->
<%= form_tag("/search", method: "get") do %>
  <%= label_tag(:power, "Search by Super Power:") %>
  <%= text_field_tag(:power) %>
  <%= submit_tag("Search") %>
<% end %>
<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine)%>
    </li>
    <br>
  <% end %>
</ul>
