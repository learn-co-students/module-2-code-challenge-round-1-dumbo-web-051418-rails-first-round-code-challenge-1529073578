<h2>All Heroines</h2>

<%= form_tag(heroines_path method: :get) do %>
  <strong>Search by Power</strong> <%= text_field_tag :power, params[:power] %>
  <%= submit_tag 'Search', name: nil %>
<% end %>


<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= heroine.super_name  %>
    </li>
    <br>
  <% end %>
</ul>


